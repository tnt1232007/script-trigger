{
  "name": "script-trigger",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"ng serve\" \"npm run electron\"",
    "build": "ng build --prod && npm run build:electron",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "tsc src/electron/electron.ts && electron ./src/electron/electron",
    "build:electron": "tsc src/electron/electron.ts --outDir dist",
    "dist:deprecate": "npm run build && npm --prefix ./dist install ./dist && cross-var electron-packager dist $npm_package_name-$npm_package_version --out=dist/packages --platform=win32 --arch=all --overwrite=true",
    "dist": "npm run build && electron-builder --dir --project dist",
    "package": "npm run build && npm --prefix ./dist install ./dist && electron-builder --project dist",
    "publish": "npm run package"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.1",
    "@angular/common": "^5.2.1",
    "@angular/compiler": "^5.2.1",
    "@angular/core": "^5.2.1",
    "@angular/forms": "^5.2.1",
    "@angular/http": "^5.2.1",
    "@angular/platform-browser": "^5.2.1",
    "@angular/platform-browser-dynamic": "^5.2.1",
    "@angular/router": "^5.2.1",
    "angular-confirmation-popover": "^4.0.0",
    "angular2-elastic": "^0.13.0",
    "angular2-moment": "^1.7.1",
    "bootstrap": "^4.0.0",
    "core-js": "^2.4.1",
    "highlight.js": "^9.12.0",
    "jquery": "^3.3.1",
    "ngx-bootstrap": "^2.0.2",
    "ngx-highlightjs": "^1.4.0",
    "popper.js": "^1.13.0",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.5",
    "@angular/compiler-cli": "^5.2.1",
    "@angular/language-service": "^5.2.1",
    "@types/jasmine": "^2.8.4",
    "@types/jasminewd2": "~2.0.2",
    "@types/jquery": "^3.2.18",
    "@types/node": "^9.3.0",
    "chokidar": "^2.0.0",
    "codelyzer": "^4.1.0",
    "concurrently": "^3.5.1",
    "cross-var": "^1.1.0",
    "electron": "^1.8.1",
    "electron-builder": "^19.55.2",
    "electron-is-dev": "^0.3.0",
    "electron-packager": "^10.1.1",
    "electron-store": "^1.3.0",
    "jasmine-core": "^2.9.1",
    "jasmine-spec-reporter": "^4.2.1",
    "jsonfile": "^4.0.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-powershell": "^3.3.1",
    "protractor": "^5.2.2",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
  }
}
