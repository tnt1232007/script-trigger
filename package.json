{
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"ng serve\" \"npm run start:electron\"",
    "build": "ng build --prod && npm run build:electron",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:electron": "tsc src/electron/electron.ts && electron ./src/electron/electron",
    "build:electron": "tsc src/electron/electron.ts --outDir dist",
    "dist": "npm run build && electron-builder --dir --project dist",
    "package": "npm run build && electron-builder --project dist",
    "publish": "npm run package"
  },
